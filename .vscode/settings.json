{
  "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt.out && $dir$fileNameWithoutExt.out",
    // 既不从文档输入，又不输出到文档
    "cpp": "cd $dir && g++ $fileName -Wall -static-libgcc -g -std=c++17 -o a.out && $dir/a.out",
    // 从文档输入
    // "cpp": "cd $dir && g++ $fileName -Wall -static-libgcc -g -std=c++17 -o a.out && $dir/a.out < input",
